div(class="col-list members")
	div(class="toggle")
		button(
			type="button",
			aria-label="toggle menu",
			aria-expanded="false",
			class="btn btn-toggle",
			data-target="hidden-members")
			span(class='line')
	div(class="members-list list")
		ul
			-for(let i =0; i<= 5; i++)
				li
					button(type='button',class='btn btn-white')= `room ${i}`